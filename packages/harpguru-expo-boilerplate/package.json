{
  "name": "harpguru-expo-boilerplate",
  "version": "1.3.0",
  "description": "An expo based boilerplate to run the harpguru-core app",
  "main": "__generated__/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "lint": "eslint --ext .ts,.tsx .",
    "ci": "yarn lint && yarn tsc",
    "prettier": "prettier --write .",
    "build-android-aad": "expo build:android -t app-bundle --clear-credentials",
    "build-android-apk": "expo build:android -t apk --clear-credentials"
  },
  "dependencies": {
    "expo": "^38.0.0",
    "expo-screen-orientation": "~1.1.1",
    "harpguru-core": "0.2.2",
    "react": "16.11.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-38.0.2.tar.gz"
  },
  "private": true
}
