{
  "name": "harpguru-expo-boilerplate",
  "version": "8.0.0",
  "description": "An expo based boilerplate to run the harpguru-core app",
  "main": "index.ts",
  "scripts": {
    "tunnel": "npx expo start --tunnel",
    "build-android": "npx eas-cli build -p android --profile production",
    "THE_FOLLOWING_WERE_JUST_AUTO_CREATED": "echo 'not a real script'",
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint": "expo lint"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "expo": "^54.0.0",
    "expo-constants": "~18.0.13",
    "expo-font": "~14.0.11",
    "expo-linking": "~8.0.11",
    "expo-router": "~6.0.23",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "expo-system-ui": "~6.0.9",
    "expo-web-browser": "~15.0.10",
    "harpguru-core": "^11.0.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "^0.21.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/jest": "^29.5.12",
    "@types/react": "~19.1.0",
    "jest": "^29.2.1",
    "jest-expo": "~54.0.0",
    "typescript": "~5.8.0"
  },
  "private": true
}
